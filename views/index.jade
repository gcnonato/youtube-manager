extends layout

block content
	h1= title
	p Welcome to #{title}
	script(type='text/template', id='job_template') <td><%= url %></td><td><%= status %></td>

	div(id='jobs')
		div(id='newjob')
			form(id='addJobForm', action='#')
				label(for='url') Url:
				input(id='url')
				br
				label(for='status') Status:
				input(id='status')
				br
				button(id='addButton') Add
			table
				thead
					td Url
					td Status
				tbody(id='jobdata')
